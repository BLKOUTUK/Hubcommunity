# Sentry DSN for error tracking
VITE_SENTRY_DSN=https://example@sentry.io/123456

# Environment
NODE_ENV=development

# API Configuration
VITE_API_URL=http://localhost:3000/api

# Supabase Configuration
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your-anon-key

# Airtable Configuration
VITE_AIRTABLE_API_KEY=your-airtable-api-key
VITE_AIRTABLE_BASE_ID=your-airtable-base-id

# Heartbeat Configuration for BLKOUTHUB Integration
VITE_HEARTBEAT_API_KEY=your-heartbeat-api-key
VITE_HEARTBEAT_API_URL=https://api.heartbeat.chat/v1
VITE_HEARTBEAT_COMMUNITY_ID=your-heartbeat-community-id
VITE_HEARTBEAT_WEBHOOK_SECRET=your-webhook-secret

# Feature Flags
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_NOTIFICATIONS=true

# The Lowdown API Configuration
VITE_LOWDOWN_API_KEY=your_lowdown_api_key
VITE_LOWDOWN_API_URL=https://api.thelowdown.com/v1
VITE_LOWDOWN_CALENDAR_ID=your_calendar_id

# Google Calendar Configuration
VITE_GOOGLE_CALENDAR_ID=your_google_calendar_id
VITE_GOOGLE_API_KEY=your_google_api_key

# Other Configuration
VITE_APP_URL=http://localhost:5173
VITE_APP_DEV_URL=http://localhost:5173